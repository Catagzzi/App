<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.Button" parent="@style/Widget.AppCompat.Button">
        <item name="android:textColor">@color/white</item>
        <item name="colorButtonNormal">@color/colorPrimaryDark</item>
        <item name="colorControlHighlight">@color/click</item>
    </style>
    <style name="AppTheme.ButtonGo" parent="@style/Widget.AppCompat.Button">
        <item name="android:textColor">@color/white</item>
        <item name="colorControlHighlight">@color/click</item>
        <item name="backgroundTint">@color/goButton</item>
    </style>


</resources>